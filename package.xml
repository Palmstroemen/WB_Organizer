  <pythonmin>3.6.0</pythonmin>

  <url type="website">https://github.com/Palmstroemen/WB_Organizer/wiki</url>

  <url branch="main" type="repository">https://github.com/Palmstroemen/WB_Organizer.git</url>

  <url type="bugtracker">https://github.com/Palmstroemen/WB_Organizer/issues</url>

